{
  "name": "@unlurk/react",
  "version": "0.1.0",
  "description": "React bindings for Unlurk",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "keywords": [
    "unlurk",
    "react",
    "engagement",
    "ai"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/unlurk/unlurk.git",
    "directory": "packages/react"
  },
  "peerDependencies": {
    "react": ">=18.2.0 || >=19.0.3",
    "react-dom": ">=18.2.0 || >=19.0.3",
    "unlurk": "^0.1.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "react": "^19.0.3",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0"
  }
}
